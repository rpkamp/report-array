{
    "name": "rpkamp/report-array",
    "description": "Easily create aggregate arrays from flat data",
    "keywords": ["array", "reporting", "aggregate"],
    "homepage": "http://github.com/rpkamp/report-array",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Remon van de Kamp",
            "email": "rpkamp@gmail.com",
            "homepage": "https://www.scallioxtx.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "jakub-onderka/php-parallel-lint": "0.*",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {"rpkamp\\ReportArray\\": "src/ReportArray"}
    },
    "autoload-dev": {
        "psr-4": {"rpkamp\\ReportArray\\": "tests/ReportArray"}
    },
    "scripts": {
        "test": [
            "parallel-lint . --exclude vendor",
            "phpunit -c phpunit.dist.xml"
        ]
    }
}
